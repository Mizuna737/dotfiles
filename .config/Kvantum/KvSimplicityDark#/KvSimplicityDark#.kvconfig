indicator.size=12
text.normal.color=#CBCCC6
text.focus.color=#A6CC70
text.toggle.color=#95E6CB
min_width=4font
min_height=1.4font

[Progressbar]
inherits=PanelButtonCommand
frame.element=progress
frame.expandedElement=eprogress
interior.element=progress
